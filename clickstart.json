{
    "id": "simpleweb4j-clickstart",
    "name": "SimpleWeb4j clickstart",
    "source": "github",
    "description" : "SimpleWeb4j application connected to a MySQL database",
    "icon" : "https://raw.github.com/ybonnel/SimpleWeb4j/master/images/logo-simpleweb4j-140.png",

    "supported-by" : "community",
    "order" : 0,

    "runtime": {
        "type": "java",
        "app-parameters" : { 
            "runtime.java_version": "1.7",
            "runtime.class": "com.mycompany.Main"
        }
        
    },
    "databases": [
        {
            "type": "mysql",
            "alias": "beers"
        }
    ],
    "build-with-jenkins": {
        "template": {
            "type": "https://raw.github.com/ybonnel/simpleweb4j-clickstart/master/jenkins.xml",
            "options": {
            }            
        }
    }
}
